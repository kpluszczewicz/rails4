<!DOCTYPE html>
<html>
<head>
  <title>Rails4</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
  </header>
    <h1>Project name</h1>
  </header>
  <nav>
    <ul>
      <li><%= link_to "Home", welcome_path %></li>
        <% if user_signed_in? %>
          <li><%= link_to "New presentation", new_presentation_path %></li>
          <li><%= link_to "Sign out", destroy_user_session_path %></li>
        <% else %>
          <li><%= link_to "Sign in", new_user_session_path %></li>
          <li><%= link_to "Sign up", new_user_registration_path %></li>
        <% end %>
      <li><%= link_to "About us", aboutus_path %></li>
    </ul>
  </nav>
  <section>
    <% if flash[:alert] != nil %>
      <p class="alert"><%= flash[:alert] %></p>
    <% end %>
    <% if flash[:warning] != nil %>
      <p class="warning"><%= flash[:warning] %></p>
    <% end %>
    <% if flash[:notice] != nil %>
      <p class="notice"><%= flash[:notice] %></p>
    <% end %>
    <%= yield %>
  </section>
  <footer>
    <p>Copyright 2011</p>
  </footer>
</body>
</html>
