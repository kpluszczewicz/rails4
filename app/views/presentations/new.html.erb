<%= form_for(@presentation, :html => { :multipart => true }) do |f| %>
    <p>
        <%= f.label 'Title' %>
        <%= f.text_field :title %>
    </p>
    <p>
        <%= f.label 'Description' %>
        <%= f.text_field :description %>
    </p>
    <p>
        <%= f.label 'File' %>
        <%= f.file_field 'filename' %>
    </p>
    <%= f.submit 'save' %>
<% end %>